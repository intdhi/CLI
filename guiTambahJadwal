    /**
     * @return the tChari
     */
    public int gettChari() {
        if ("senin".equals(tChari.getSelectedItem().toString())) {
            return 1;
        }else if ("selasa".equals(tChari.getSelectedItem().toString())){
            return 2;
        }else if ("rabu".equals(tChari.getSelectedItem().toString())){
            return 3;
        }else if ("kamis".equals(tChari.getSelectedItem().toString())){
            return 4;
        }else if ("jumat".equals(tChari.getSelectedItem().toString())){
            return 5;
        }else {
            return 6;
        }
    }

    /**
     * @return the tCjam
     */
    public int gettCjam() {
        return tCjam.getSelectedIndex()+1;
    }

    /**
     * @return the tDosen
     */
    public String gettDosen() {
        return tDosen.getText();
    }

    /**
     * @return the tIdJadwal
     */
    public String gettIdJadwal() {
        return tIdJadwal.getText();
    }

    /**
     * @return the tIdRuang
     */
    public String gettIdRuang() {
        return tIdRuang.getText();
    }

    /**
     * @return the tKelas
     */
    public String gettKelas() {
        return tKelas.getText();
    }

    /**
     * @return the tMK
     */
    public String gettMK() {
        return tMK.getText();
    }

    /**
     * @return the tTambah
     */
    public javax.swing.JButton gettTambah() {
        return tTambah;
    }

    /**
     * @return the back
     */
    public Object getBack() {
        return back;
    }

